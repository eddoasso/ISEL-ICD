<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>JSP-ExamplesWebApp</display-name>
  <listener>
    <listener-class>
        serverData.ServerData
    </listener-class>
  </listener>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>ServletX</servlet-name>
    <servlet-class>answersCorrection.WriteAnswersAsync</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>ServletX</servlet-name>
    <url-pattern>/servletAjax</url-pattern>
</servlet-mapping>
<servlet>
    <servlet-name>ServletZ</servlet-name>
    <servlet-class>answersCorrection.WriteStudentsConnected</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>ServletZ</servlet-name>
    <url-pattern>/servletAjaxStudentsCon</url-pattern>
</servlet-mapping>
<servlet>
    <servlet-name>ServletY</servlet-name>
    <servlet-class>submitQuestions.ShowCounterQuestAsync</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>ServletY</servlet-name>
    <url-pattern>/servletAjaxCountAnswers</url-pattern>
</servlet-mapping>
<servlet>
    <servlet-name>ServletR</servlet-name>
    <servlet-class>loginProf.ProfOnlineAsync</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>ServletR</servlet-name>
    <url-pattern>/servletShowOnProf</url-pattern>
</servlet-mapping>
<servlet>
    <servlet-name>ServletI</servlet-name>
    <servlet-class>submitQuestions.ShowStudentsAnswersWaiting</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>ServletI</servlet-name>
    <url-pattern>/servletPendentQuest</url-pattern>
</servlet-mapping>
</web-app>